{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "no-alert": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "consistent-return": "off"
    }
}
